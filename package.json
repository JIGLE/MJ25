{
  "name": "mj25-wedding-website",
  "private": true,
  "version": "1.0.0",
  "description": "Marlene & Jose's Wedding Website",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "dev:network": "vite --host 0.0.0.0 --port 3000",
    "build": "vite build",
    "preview": "vite preview --host 0.0.0.0",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "clean": "rm -rf dist node_modules/.vite",
    "fresh-install": "rm -rf node_modules package-lock.json && npm install",
    "docker:build": "docker build -t mj25-wedding .",
    "docker:run": "docker run -p 30080:80 mj25-wedding",
    "deploy:local": "npm run build && docker-compose up -d --build"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "i18next": "^22.4.0",
    "react-i18next": "^12.1.0",
    "i18next-browser-languagedetector": "^7.0.0",
    "firebase": "^9.17.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.34.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "sass": "^1.58.0",
    "vite": "^4.1.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
